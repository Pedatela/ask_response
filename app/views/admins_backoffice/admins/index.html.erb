<div class="row p-3">
  <div class="col-lg-12">
    <h1 class="page-header">Listando Administradores</h1>
  </div>
</div>
<div class="row">
  <table id="datatablesSimple" class="dataTable-table">
    <thead>
      <tr>
        <th>
          <a href="#">Id</a>
        </th>
        <th >
          <a href="#">Email</a>
        </th>
        <th >
          <a href="#"></a>
        </th>
      </tr>
    </thead>
    <tbody>
      <% @admins.each do |admin|  %>
        <tr>
          <td><%= admin.id %></td>
          <td><%= admin.email %></td>
          <td>
            <%= link_to edit_admins_backoffice_admin_path(admin), class: "btn btn-primary btn-sm rouded-5"  do%>
              <i class="fa fa-edit"></i>
            <%end%>
            <button class="btn btn-danger btn-sm" type="button" data-toggle="tooltip" data-placement="top" title="Delete">
              <i class="fa fa-trash"></i>
            </button>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>