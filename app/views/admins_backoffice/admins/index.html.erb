<h1>Listando Administradores</h1>
<table id="datatablesSimple" class="dataTable-table">
  <thead>
    <tr>
      <th>
        <a href="#">Id</a>
      </th>
      <th >
        <a href="#">Email</a>
      </th>
      <th >
        <a href="#"></a>
      </th>
    </tr>
  </thead>
  <tbody>
    <% @admins.each do |admin|  %>
      <tr>
        <td><%= admin.id %></td>
        <td><%= admin.email %></td>
        <td>
          <button class="btn btn-primary btn-sm btn-circle" type="button" data-toggle="tooltip" data-placement="top" title="Edit">
            <i class="fa fa-edit"></i>
          </button>
          <button class="btn btn-danger btn-sm btn-circle" type="button" data-toggle="tooltip" data-placement="top" title="Delete">
            <i class="fa fa-trash"></i>
          </button>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>